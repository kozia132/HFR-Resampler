{
    "framerate": 60, 
    "resolution": "UNCHANGED", 
    "fourcc": "mp4v", 
    "cv_colourfix": false,
    "blend_range": 1.0, 
    "blend_mode": "EQUAL",
    "use_ffmpeg_encoder": true,
    "encoder_settings": {
        "encoder": "h264_nvenc",
        "preset": "fast",
        "crf": 18,
        "pixel_format": "yuv420p",
        "extra_params": []
    },
    "output_suffix": "_60fps",
    "output_extension": ".mkv",
    "blend_settings": {
        "pyramid": {
            "reverse": false
        },
        "gaussian": {
            "standard_deviation": 2,
            "bound": [0,2]
        },
        "custom_function": {
            "function": "-x**2+1", 
            "bound": [-0.5,1]
        },
        "custom_weight":{
            "weight": [1,3,2,2]
        }
    }
}
